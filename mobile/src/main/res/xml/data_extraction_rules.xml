<?xml version="1.0" encoding="utf-8"?><!--
   Sample data extraction rules file; uncomment and customize as necessary.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Include app data that should be backed up -->
        <include domain="database" path="match_database" />
        <include domain="database" path="match_database-shm" />
        <include domain="database" path="match_database-wal" />
        <include domain="sharedpref" path="."/>
        <!-- Exclude temporary or sensitive data -->
        <exclude domain="no-backup" path="."/>
        <exclude domain="cache" path="."/>
        <exclude domain="external" path="."/>
    </cloud-backup>
    <!--
    <device-transfer>
        <include .../>
        <exclude .../>
    </device-transfer>
    -->
</data-extraction-rules>